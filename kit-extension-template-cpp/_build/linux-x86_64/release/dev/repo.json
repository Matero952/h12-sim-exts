{
    "repo": {
        "extra_tool_paths": [
            "${root}/../kit/dev"
        ],
        "tokens": {
            "abi": "2.35"
        }
    },
    "repo_precache_exts": {
        "enabled": true,
        "kit_path": "${root}/../kit/kit${exe_ext}",
        "kit_extra_args": [
            "--/app/enableStdoutOutput=0",
            "--/app/fastShutdown=1"
        ],
        "kit_portable": false,
        "cache_path": "${root}/../extscache",
        "ext_folders": [
            "${root}/../exts",
            "${root}/../apps"
        ],
        "kit_parallel_pull": true,
        "kit_omit_ext_version": false,
        "apps": [],
        "generated_app_path": "${root}/../apps/exts.deps.generated.kit",
        "publish_artifacts": [],
        "registries": [
            {
                "name": "kit/default",
                "url": "${env:PULL_EXTENSIONS_REGISTRY_URL_0}"
            },
            {
                "name": "kit/sdk",
                "url": "${env:PULL_EXTENSIONS_REGISTRY_URL_1}"
            }
        ]
    },
    "repo_kit_pull_extensions": {
        "enabled": true,
        "build_config": "release",
        "precache_exts_enabled": true,
        "skip_pull_if_paths_exist": [
            "${root}/../kit"
        ],
        "registries": {
            "internal": [
                {
                    "name": "kit/default",
                    "url": "omniverse://kit-extensions.ov.nvidia.com/exts/kit/default"
                },
                {
                    "name": "kit/sdk",
                    "url": "omniverse://kit-extensions.ov.nvidia.com/exts/kit/sdk/${kit_version_short}/${kit_git_hash}"
                }
            ],
            "public": [
                {
                    "name": "kit/default",
                    "url": "omniverse://kit-extensions.ov.nvidia.com/exts/kit/default"
                },
                {
                    "name": "kit/sdk",
                    "url": "omniverse://kit-extensions.ov.nvidia.com/exts/kit/sdk/${kit_version_short}/${kit_git_hash}"
                }
            ]
        }
    }
}